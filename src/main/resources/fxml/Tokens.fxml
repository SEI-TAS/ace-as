<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.cell.PropertyValueFactory?>

<GridPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
          fx:controller="edu.cmu.sei.ttg.aaiot.as.gui.controllers.TokensController"
          alignment="TOP_LEFT" hgap="10" vgap="10">
<padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
</padding>
<TableView fx:id="tokensTableView" GridPane.columnIndex="0" GridPane.rowIndex="2">
    <columns>
        <TableColumn text="ID" prefWidth="120">
            <cellValueFactory>
                <PropertyValueFactory property="id" />
            </cellValueFactory>
        </TableColumn>
        <TableColumn text="Client">
            <cellValueFactory>
                <PropertyValueFactory property="clientId" />
            </cellValueFactory>
        </TableColumn>
        <TableColumn text="Resource Server (Device)">
            <cellValueFactory>
                <PropertyValueFactory property="rsId" />
            </cellValueFactory>
        </TableColumn>
        <TableColumn text="Actions">
        </TableColumn>
    </columns>
</TableView>
</GridPane>